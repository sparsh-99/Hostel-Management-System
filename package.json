  "bugs": {
    "url": "https://github.com/starkblaze01/Hostel-Management/issues"
  },
  "homepage": "https://github.com/starkblaze01/Hostel-Management#readme",
  "dependencies": {
    "axios": "^0.21.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "classnames": "^2.3.1",
    "concurrently": "^5.3.0",
    "express": "^4.16.4",
    "gravatar": "^1.8.1",
    "jsonwebtoken": "^8.5.1",
    "jwt-decode": "^3.1.2",
    "mongoose": "^5.12.8",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-loading": "^2.0.3",
    "react-loading-skeleton": "^2.2.0",
